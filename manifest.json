{
  "manifest_version": 2,
  "name": "BrowsePass",
  "description": "A read-only, web-enabled version of KeePass",
  "version": "0.7",
  "app": {
    "background": {
      "scripts": ["chrome-app.js"]
    }
  },
  "key": "",
  "oauth2": {
    "client_id": "595395511986-papjag05ej3ailoqqoj01lfhvfvsf5pe.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive.readonly"
    ]
  },
  "permissions": [
